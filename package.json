{
	"name": "akos",
	"displayName": "Akos Help",
	"description": "a vscode plugin for akos",
	"version": "1.0.1",
	"publisher": "Alps",
	"engines": {
		"vscode": "^1.0.0"
	},
	"galleryBanner": {
		"color": "#457cd0",
		"theme": "dark"
	  },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.akos"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.akos",
				"title": "akos",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				  }
			}
		],
		"menus": {
			"editor/title": [{
				"when": "resourceLangId == javascript",
				"command": "extension.akos",
				"group": "navigation"
			}]
		},
		"keybindings": [
			{
				"key": "ctrl+alt+l",
				"mac": "ctrl+alt+l",
				"command": "extension.akos"
			}
		],
		"configuration": {
			"type": "object",
			"title": "vscode-plugin-akos",
			"properties": {
				"vscodePluginAkos.identifier": {
					"type": "string",
					"default": ": ",
					"description": "描述与值的分隔符，默认: "
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}